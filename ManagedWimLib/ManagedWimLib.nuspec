<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ManagedWimLib</id>
    <title>ManagedWimLib</title>
    <version>1.3.0</version>
    <owners>ied206</owners>
    <authors>Eric Biggers, Hajin Jang</authors>
    <projectUrl>https://github.com/ied206/ManagedWimLib</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/ied206/ManagedWimLib/master/Image/Logo.png</iconUrl>
    <license type="expression">LGPL-3.0-or-later</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>C# pinvoke library for wimlib.
Supports Windows and Linux.</description>
    <summary>C# pinvoke library for wimlib.</summary>
    <releaseNotes>- Update included wimlib to 1.13.1
- Support dll with additional dependency
- Drop .Net Standard 1.3 support</releaseNotes>
    <copyright>Copyright (c) 2018-2019 Hajin Jang</copyright>
    <tags>wim wimlib imagex pinvoke interop</tags>
    <dependencies>
      <group targetFramework="net451"></group>
      <group targetFramework="netstandard2.0"></group>
    </dependencies>
  </metadata>
  <files>
    <!-- .Net Framework 4.5.1 -->
    <file src="ManagedWimLib.targets" target="build\net451\ManagedWimLib.targets" />
    <file src="bin\Release\net451\ManagedWimLib.dll" target="lib\net451" />
    <file src="bin\Release\net451\ManagedWimLib.pdb" target="lib\net451" />
    <file src="Precompiled\x86\*.dll" target="build\net451\Precompiled\x86" />
    <file src="Precompiled\x64\*.dll" target="build\net451\Precompiled\x64" />
    <!-- .Net Standard 2.0 -->
    <file src="ManagedWimLib.targets" target="build\netstandard2.0\ManagedWimLib.targets" />
    <file src="bin\Release\netstandard2.0\ManagedWimLib.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\ManagedWimLib.pdb" target="lib\netstandard2.0" />
    <file src="Precompiled\x86\*" target="build\netstandard2.0\Precompiled\x86" />
    <file src="Precompiled\x64\*" target="build\netstandard2.0\Precompiled\x64" />
    <file src="Precompiled\armhf\*" target="build\netstandard2.0\Precompiled\armhf" />
    <file src="Precompiled\arm64\*" target="build\netstandard2.0\Precompiled\arm64" />
  </files>
